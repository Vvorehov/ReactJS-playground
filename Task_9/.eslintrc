{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "env": {
    "browser": true,
    "jest": true
  },
  "plugins": ["react", "jest"],
  "rules": {
    "max-len": [1, 120, 2],
    "prop-types": [2],
    "linebreak-style": 0
  },
  "extends": ["airbnb-base"]
}